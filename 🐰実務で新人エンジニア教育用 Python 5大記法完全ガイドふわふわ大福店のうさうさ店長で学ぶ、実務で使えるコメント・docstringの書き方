        """
        if stock < 0:
            raise ValueError("åœ¨åº«æ•°ã¯0ä»¥ä¸Šã«ã—ã¦ãã ã•ã„")
        
        self.owner_name = owner_name
        self.stock = stock
        self.sold = 0
        self.revenue = 0
    
    def sell(
        self,
        quantity: int,
        price: int = 150,
        discount: float = 0
    ) -> Optional[int]:
        """
        å¤§ç¦ã‚’è²©å£²ã™ã‚‹
        
