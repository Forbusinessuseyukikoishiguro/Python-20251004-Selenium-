def calculate_shipping_fee(items, weight, distance):
    if len(items) > 10 and weight < 5000:
        base = 500
    elif distance > 100:
        base = 1000 + (distance - 100) * 10
    else:
        base = 800
    
    if weight > 3000:
        base += (weight - 3000) // 1000 * 200
    
    return base

