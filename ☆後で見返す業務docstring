print("=" * 70)
print("ã€ãƒ‘ã‚¿ãƒ¼ãƒ³1ã€‘PEP 257 â†’ ä»–ã®4è¨˜æ³•ã¸æ›¸ãæ›ãˆ")
print("=" * 70)


# ----- å…ƒ: PEP 257 -----
def calc_original_pep(x, y):
    """2ã¤ã®æ•°ã‚’è¶³ã—ç®—ã™ã‚‹ã€‚"""
    # ã€ã“ã®é–¢æ•°ã®ç›®çš„ã€‘
    # - 2ã¤ã®æ•´æ•°ã‚’å—ã‘å–ã‚‹
    # - è¶³ã—ç®—ã™ã‚‹
    # - çµæœã‚’è¿”ã™

    print(f"  [PEP257å…ƒ] å®Ÿè¡Œ: {x} + {y}")  # ãƒ‡ãƒãƒƒã‚°
    result = x + y  # è¨ˆç®—
    print(f"  [PEP257å…ƒ] çµæœ: {result}")  # ãƒ‡ãƒãƒƒã‚°
    return result


print("\n--- PEP 257 â†’ Google ã¸ã®æ›¸ãæ›ãˆ ---")


def calc_pep_to_google(x, y):
    """
    2ã¤ã®æ•°ã‚’è¶³ã—ç®—ã™ã‚‹ã€‚

    Args:
        x (int): 1ã¤ç›®ã®æ•°
        y (int): 2ã¤ç›®ã®æ•°

    Returns:
        int: x + y ã®çµæœ
    """
    # ã€æ›¸ãæ›ãˆå†…å®¹ã€‘
    # PEP 257: """2ã¤ã®æ•°ã‚’è¶³ã—ç®—ã™ã‚‹ã€‚"""
    #    â†“
    # Google: æ¦‚è¦ + Args + Returns

    print(f"  [Googleç‰ˆ] å®Ÿè¡Œ: {x} + {y}")
    print(f"  [Googleç‰ˆ] å¼•æ•°x={x}, y={y} ã‚’è¶³ã—ç®—")
    result = x + y
    print(f"  [Googleç‰ˆ] çµæœ: {result}")
    print(f"  [æ›¸ãæ›ãˆæˆåŠŸ] PEP 257 â†’ Google")
    return result


print("\n--- PEP 257 â†’ NumPy ã¸ã®æ›¸ãæ›ãˆ ---")


def calc_pep_to_numpy(x, y):
    """
    2ã¤ã®æ•°ã‚’è¶³ã—ç®—ã™ã‚‹ã€‚

    Parameters
    ----------
    x : int
        1ã¤ç›®ã®æ•°
    y : int
        2ã¤ç›®ã®æ•°

    Returns
    -------
    int
        x + y ã®çµæœ
    """
    # ã€æ›¸ãæ›ãˆå†…å®¹ã€‘
    # PEP 257: """2ã¤ã®æ•°ã‚’è¶³ã—ç®—ã™ã‚‹ã€‚"""
    #    â†“
    # NumPy: æ¦‚è¦ + Parameters + ãƒã‚¤ãƒ•ãƒ³ + Returns

    print(f"  [NumPyç‰ˆ] å®Ÿè¡Œ: {x} + {y}")
    print(f"  [NumPyç‰ˆ] Parameterså½¢å¼ã§å¼•æ•°ã‚’è¨˜è¿°")
    result = x + y
    print(f"  [NumPyç‰ˆ] çµæœ: {result}")
    print(f"  [æ›¸ãæ›ãˆæˆåŠŸ] PEP 257 â†’ NumPy")
    return result


print("\n--- PEP 257 â†’ reST ã¸ã®æ›¸ãæ›ãˆ ---")


def calc_pep_to_rest(x, y):
    """
    2ã¤ã®æ•°ã‚’è¶³ã—ç®—ã™ã‚‹ã€‚!

    :param x: 1ã¤ç›®ã®æ•°
    :type x: int
    :param y: 2ã¤ç›®ã®æ•°
    :type y: int
    :return: x + y ã®çµæœ
    :rtype: int
    """
    # ã€æ›¸ãæ›ãˆå†…å®¹ã€‘
    # PEP 257: """2ã¤ã®æ•°ã‚’è¶³ã—ç®—ã™ã‚‹ã€‚"""
    #    â†“
    # reST: æ¦‚è¦ + :param/:type + :return/:rtype

    print(f"  [reSTç‰ˆ] å®Ÿè¡Œ: {x} + {y}")
    print(f"  [reSTç‰ˆ] :param ã¨ :type ã§å¼•æ•°ã‚’2è¡Œãšã¤è¨˜è¿°")
    result = x + y
    print(f"  [reSTç‰ˆ] çµæœ: {result}")
    print(f"  [æ›¸ãæ›ãˆæˆåŠŸ] PEP 257 â†’ reST")
    return result


print("\n--- PEP 257 â†’ Epytext ã¸ã®æ›¸ãæ›ãˆ ---")


def calc_pep_to_epytext(x, y):
    """
    2ã¤ã®æ•°ã‚’è¶³ã—ç®—ã™ã‚‹ã€‚

    @param x: 1ã¤ç›®ã®æ•°
    @type x: int
    @param y: 2ã¤ç›®ã®æ•°
    @type y: int
    @return: x + y ã®çµæœ
    @rtype: int
    """
    # ã€æ›¸ãæ›ãˆå†…å®¹ã€‘
    # PEP 257: """2ã¤ã®æ•°ã‚’è¶³ã—ç®—ã™ã‚‹ã€‚"""
    #    â†“
    # Epytext: æ¦‚è¦ + @param/@type + @return/@rtype

    print(f"  [Epytextç‰ˆ] å®Ÿè¡Œ: {x} + {y}")
    print(f"  [Epytextç‰ˆ] @ è¨˜æ³•ã§è¨˜è¿°ï¼ˆãƒ¬ã‚¬ã‚·ãƒ¼ï¼‰")
    result = x + y
    print(f"  [Epytextç‰ˆ] çµæœ: {result}")
    print(f"  [æ›¸ãæ›ãˆæˆåŠŸ] PEP 257 â†’ Epytext")
    return result


# ãƒ†ã‚¹ãƒˆå®Ÿè¡Œ
print("\nâ–¼ ãƒ‘ã‚¿ãƒ¼ãƒ³1ã®ãƒ†ã‚¹ãƒˆå®Ÿè¡Œ")
x, y = 10, 5
print(f"å…¥åŠ›å€¤: x={x}, y={y}\n")

print("1-0. å…ƒã®PEP 257:")
calc_original_pep(x, y)

print("\n1-1. PEP 257 â†’ Google:")
calc_pep_to_google(x, y)

print("\n1-2. PEP 257 â†’ NumPy:")
calc_pep_to_numpy(x, y)

print("\n1-3. PEP 257 â†’ reST:")
calc_pep_to_rest(x, y)

print("\n1-4. PEP 257 â†’ Epytext:")
calc_pep_to_epytext(x, y)


# ====================================
# ãƒ‘ã‚¿ãƒ¼ãƒ³2: Google ã‹ã‚‰æ›¸ãæ›ãˆ
# ====================================

print("\n\n" + "=" * 70)
print("ã€ãƒ‘ã‚¿ãƒ¼ãƒ³2ã€‘Google â†’ ä»–ã®4è¨˜æ³•ã¸æ›¸ãæ›ãˆ")
print("=" * 70)


# ----- å…ƒ: Google -----
def calc_original_google(x, y, z=10):
    """
    (x / y) + z ã‚’è¨ˆç®—ã™ã‚‹ã€‚

    Args:
        x (float): å‰²ã‚‰ã‚Œã‚‹æ•°
        y (float): å‰²ã‚‹æ•°
        z (float, optional): åŠ ç®—ã™ã‚‹æ•°ã€‚ãƒ‡ãƒ•ã‚©ãƒ«ãƒˆã¯10

    Returns:
        float: è¨ˆç®—çµæœ

    Raises:
        ValueError: yãŒ0ã®å ´åˆ
    """
    # ã€ã“ã®é–¢æ•°ã®ç›®çš„ã€‘
    # - xã‚’yã§å‰²ã‚‹
    # - zã‚’è¶³ã™
    # - yãŒ0ãªã‚‰ã‚¨ãƒ©ãƒ¼

    print(f"  [Googleå…ƒ] å®Ÿè¡Œ: ({x} / {y}) + {z}")
    if y == 0:
        print(f"  [Googleå…ƒ] ã‚¨ãƒ©ãƒ¼: yãŒ0ã§ã™")
        raise ValueError("yã¯0ä»¥å¤–")

    result = x / y + z
    print(f"  [Googleå…ƒ] çµæœ: {result}")
    return result


print("\n--- Google â†’ PEP 257 ã¸ã®æ›¸ãæ›ãˆ ---")


def calc_google_to_pep(x, y, z=10):
    """(x / y) + z ã‚’è¨ˆç®—ã™ã‚‹ã€‚"""
    # ã€æ›¸ãæ›ãˆå†…å®¹ã€‘
    # Google: æ¦‚è¦ + Args + Returns + Raises
    #    â†“
    # PEP 257: æ¦‚è¦ã®ã¿ï¼ˆè©³ç´°æƒ…å ±ã¯å‰Šé™¤ï¼‰

    print(f"  [PEP257ç‰ˆ] å®Ÿè¡Œ: ({x} / {y}) + {z}")
    print(f"  [PEP257ç‰ˆ] ç°¡ç•¥åŒ–ï¼ˆå¼•æ•°ãƒ»æˆ»ã‚Šå€¤ã®èª¬æ˜ã‚’å‰Šé™¤ï¼‰")

    if y == 0:
        print(f"  [PEP257ç‰ˆ] ã‚¨ãƒ©ãƒ¼: yãŒ0ã§ã™")
        raise ValueError("yã¯0ä»¥å¤–")

    result = x / y + z
    print(f"  [PEP257ç‰ˆ] çµæœ: {result}")
    print(f"  [æ›¸ãæ›ãˆæˆåŠŸ] Google â†’ PEP 257")
    return result


print("\n--- Google â†’ NumPy ã¸ã®æ›¸ãæ›ãˆ ---")


def calc_google_to_numpy(x, y, z=10):
    """
    (x / y) + z ã‚’è¨ˆç®—ã™ã‚‹ã€‚

    Parameters
    ----------
    x : float
        å‰²ã‚‰ã‚Œã‚‹æ•°
    y : float
        å‰²ã‚‹æ•°
    z : float, optional
        åŠ ç®—ã™ã‚‹æ•°ã€‚ãƒ‡ãƒ•ã‚©ãƒ«ãƒˆã¯10

    Returns
    -------
    float
        è¨ˆç®—çµæœ

    Raises
    ------
    ValueError
        yãŒ0ã®å ´åˆ
    """
    # ã€æ›¸ãæ›ãˆå†…å®¹ã€‘
    # Google: Args: + Returns: + Raises:
    #    â†“
    # NumPy: Parameters + ãƒã‚¤ãƒ•ãƒ³ + Returns + Raises

    print(f"  [NumPyç‰ˆ] å®Ÿè¡Œ: ({x} / {y}) + {z}")
    print(f"  [NumPyç‰ˆ] Argsâ†’Parameters, ãƒã‚¤ãƒ•ãƒ³è¿½åŠ , ã‚¤ãƒ³ãƒ‡ãƒ³ãƒˆå½¢å¼")

    if y == 0:
        print(f"  [NumPyç‰ˆ] ã‚¨ãƒ©ãƒ¼: yãŒ0ã§ã™")
        raise ValueError("yã¯0ä»¥å¤–")

    result = x / y + z
    print(f"  [NumPyç‰ˆ] çµæœ: {result}")
    print(f"  [æ›¸ãæ›ãˆæˆåŠŸ] Google â†’ NumPy")
    return result


print("\n--- Google â†’ reST ã¸ã®æ›¸ãæ›ãˆ ---")


def calc_google_to_rest(x, y, z=10):
    """
    (x / y) + z ã‚’è¨ˆç®—ã™ã‚‹ã€‚

    :param x: å‰²ã‚‰ã‚Œã‚‹æ•°
    :type x: float
    :param y: å‰²ã‚‹æ•°
    :type y: float
    :param z: åŠ ç®—ã™ã‚‹æ•°ï¼ˆãƒ‡ãƒ•ã‚©ãƒ«ãƒˆ: 10ï¼‰
    :type z: float
    :return: è¨ˆç®—çµæœ
    :rtype: float
    :raises ValueError: yãŒ0ã®å ´åˆ
    """
    # ã€æ›¸ãæ›ãˆå†…å®¹ã€‘
    # Google: x (float): èª¬æ˜
    #    â†“
    # reST: :param x: èª¬æ˜ + :type x: floatï¼ˆ2è¡Œã«åˆ†å‰²ï¼‰

    print(f"  [reSTç‰ˆ] å®Ÿè¡Œ: ({x} / {y}) + {z}")
    print(f"  [reSTç‰ˆ] å„å¼•æ•°ã‚’ :param ã¨ :type ã®2è¡Œã§è¨˜è¿°")

    if y == 0:
        print(f"  [reSTç‰ˆ] ã‚¨ãƒ©ãƒ¼: yãŒ0ã§ã™")
        raise ValueError("yã¯0ä»¥å¤–")

    result = x / y + z
    print(f"  [reSTç‰ˆ] çµæœ: {result}")
    print(f"  [æ›¸ãæ›ãˆæˆåŠŸ] Google â†’ reST")
    return result


print("\n--- Google â†’ Epytext ã¸ã®æ›¸ãæ›ãˆ ---")


def calc_google_to_epytext(x, y, z=10):
    """
    (x / y) + z ã‚’è¨ˆç®—ã™ã‚‹ã€‚

    @param x: å‰²ã‚‰ã‚Œã‚‹æ•°
    @type x: float
    @param y: å‰²ã‚‹æ•°
    @type y: float
    @param z: åŠ ç®—ã™ã‚‹æ•°ï¼ˆãƒ‡ãƒ•ã‚©ãƒ«ãƒˆ: 10ï¼‰
    @type z: float
    @return: è¨ˆç®—çµæœ
    @rtype: float
    @raise ValueError: yãŒ0ã®å ´åˆ
    """
    # ã€æ›¸ãæ›ãˆå†…å®¹ã€‘
    # reST: :param, :type, :return, :rtype, :raises
    #    â†“
    # Epytext: @param, @type, @return, @rtype, @raise

    print(f"  [Epytextç‰ˆ] å®Ÿè¡Œ: ({x} / {y}) + {z}")
    print(f"  [Epytextç‰ˆ] : ã‚’ @ ã«ç½®ãæ›ãˆ")

    if y == 0:
        print(f"  [Epytextç‰ˆ] ã‚¨ãƒ©ãƒ¼: yãŒ0ã§ã™")
        raise ValueError("yã¯0ä»¥å¤–")

    result = x / y + z
    print(f"  [Epytextç‰ˆ] çµæœ: {result}")
    print(f"  [æ›¸ãæ›ãˆæˆåŠŸ] Google â†’ Epytext")
    return result


# ãƒ†ã‚¹ãƒˆå®Ÿè¡Œ
print("\nâ–¼ ãƒ‘ã‚¿ãƒ¼ãƒ³2ã®ãƒ†ã‚¹ãƒˆå®Ÿè¡Œ")
x2, y2, z2 = 20.0, 4.0, 10.0
print(f"å…¥åŠ›å€¤: x={x2}, y={y2}, z={z2}\n")

print("2-0. å…ƒã®Google:")
calc_original_google(x2, y2, z2)

print("\n2-1. Google â†’ PEP 257:")
calc_google_to_pep(x2, y2, z2)

print("\n2-2. Google â†’ NumPy:")
calc_google_to_numpy(x2, y2, z2)

print("\n2-3. Google â†’ reST:")
calc_google_to_rest(x2, y2, z2)

print("\n2-4. Google â†’ Epytext:")
calc_google_to_epytext(x2, y2, z2)


# ====================================
# ãƒ‘ã‚¿ãƒ¼ãƒ³3: NumPy ã‹ã‚‰æ›¸ãæ›ãˆ
# ====================================

print("\n\n" + "=" * 70)
print("ã€ãƒ‘ã‚¿ãƒ¼ãƒ³3ã€‘NumPy â†’ ä»–ã®4è¨˜æ³•ã¸æ›¸ãæ›ãˆ")
print("=" * 70)


# ----- å…ƒ: NumPy -----
def calc_original_numpy(values):
    """
    ãƒªã‚¹ãƒˆã®çµ±è¨ˆã‚’è¨ˆç®—ã™ã‚‹ã€‚

    Parameters
    ----------
    values : list of float
        æ•°å€¤ã®ãƒªã‚¹ãƒˆ

    Returns
    -------
    mean : float
        å¹³å‡å€¤
    total : float
        åˆè¨ˆå€¤
    """
    # ã€ã“ã®é–¢æ•°ã®ç›®çš„ã€‘
    # - ãƒªã‚¹ãƒˆã‚’å—ã‘å–ã‚‹
    # - å¹³å‡ã¨åˆè¨ˆã‚’è¨ˆç®—
    # - ã‚¿ãƒ—ãƒ«ã§è¿”ã™

    print(f"  [NumPyå…ƒ] å®Ÿè¡Œ: values={values}")
    total = sum(values)
    mean = total / len(values)
    print(f"  [NumPyå…ƒ] åˆè¨ˆ={total}, å¹³å‡={mean}")
    return mean, total


print("\n--- NumPy â†’ PEP 257 ã¸ã®æ›¸ãæ›ãˆ ---")


def calc_numpy_to_pep(values):
    """ãƒªã‚¹ãƒˆã®çµ±è¨ˆã‚’è¨ˆç®—ã™ã‚‹ã€‚"""
    # ã€æ›¸ãæ›ãˆå†…å®¹ã€‘
    # NumPy: Parameters + ãƒã‚¤ãƒ•ãƒ³ + Returns
    #    â†“
    # PEP 257: æ¦‚è¦ã®ã¿

    print(f"  [PEP257ç‰ˆ] å®Ÿè¡Œ: values={values}")
    print(f"  [PEP257ç‰ˆ] è©³ç´°å‰Šé™¤ï¼ˆæ¦‚è¦ã®ã¿æ®‹ã™ï¼‰")

    total = sum(values)
    mean = total / len(values)
    print(f"  [PEP257ç‰ˆ] åˆè¨ˆ={total}, å¹³å‡={mean}")
    print(f"  [æ›¸ãæ›ãˆæˆåŠŸ] NumPy â†’ PEP 257")
    return mean, total


print("\n--- NumPy â†’ Google ã¸ã®æ›¸ãæ›ãˆ ---")


def calc_numpy_to_google(values):
    """
    ãƒªã‚¹ãƒˆã®çµ±è¨ˆã‚’è¨ˆç®—ã™ã‚‹ã€‚

    Args:
        values (list of float): æ•°å€¤ã®ãƒªã‚¹ãƒˆ

    Returns:
        tuple: (mean, total) ã®ã‚¿ãƒ—ãƒ«
            mean (float): å¹³å‡å€¤
            total (float): åˆè¨ˆå€¤
    """
    # ã€æ›¸ãæ›ãˆå†…å®¹ã€‘
    # NumPy: values : list of float (æ¬¡ã®è¡Œã«èª¬æ˜)
    #    â†“
    # Google: values (list of float): èª¬æ˜ï¼ˆ1è¡Œã«ã¾ã¨ã‚ã‚‹ï¼‰

    print(f"  [Googleç‰ˆ] å®Ÿè¡Œ: values={values}")
    print(f"  [Googleç‰ˆ] ãƒã‚¤ãƒ•ãƒ³å‰Šé™¤, 1è¡Œå½¢å¼ã«å¤‰æ›")

    total = sum(values)
    mean = total / len(values)
    print(f"  [Googleç‰ˆ] åˆè¨ˆ={total}, å¹³å‡={mean}")
    print(f"  [æ›¸ãæ›ãˆæˆåŠŸ] NumPy â†’ Google")
    return mean, total


print("\n--- NumPy â†’ reST ã¸ã®æ›¸ãæ›ãˆ ---")


def calc_numpy_to_rest(values):
    """
    ãƒªã‚¹ãƒˆã®çµ±è¨ˆã‚’è¨ˆç®—ã™ã‚‹ã€‚

    :param values: æ•°å€¤ã®ãƒªã‚¹ãƒˆ
    :type values: list of float
    :return: (mean, total) ã®ã‚¿ãƒ—ãƒ«
    :rtype: tuple
    """
    # ã€æ›¸ãæ›ãˆå†…å®¹ã€‘
    # NumPy: values : list of float (ãƒã‚¤ãƒ•ãƒ³ + ã‚¤ãƒ³ãƒ‡ãƒ³ãƒˆ)
    #    â†“
    # reST: :param values: + :type values: (2è¡Œå½¢å¼)

    print(f"  [reSTç‰ˆ] å®Ÿè¡Œ: values={values}")
    print(f"  [reSTç‰ˆ] ãƒã‚¤ãƒ•ãƒ³å‰Šé™¤, :param/:typeå½¢å¼")

    total = sum(values)
    mean = total / len(values)
    print(f"  [reSTç‰ˆ] åˆè¨ˆ={total}, å¹³å‡={mean}")
    print(f"  [æ›¸ãæ›ãˆæˆåŠŸ] NumPy â†’ reST")
    return mean, total


print("\n--- NumPy â†’ Epytext ã¸ã®æ›¸ãæ›ãˆ ---")


def calc_numpy_to_epytext(values):
    """
    ãƒªã‚¹ãƒˆã®çµ±è¨ˆã‚’è¨ˆç®—ã™ã‚‹ã€‚

    @param values: æ•°å€¤ã®ãƒªã‚¹ãƒˆ
    @type values: list of float
    @return: (mean, total) ã®ã‚¿ãƒ—ãƒ«
    @rtype: tuple
    """
    # ã€æ›¸ãæ›ãˆå†…å®¹ã€‘
    # reST: :param, :type, :return, :rtype
    #    â†“
    # Epytext: @param, @type, @return, @rtype

    print(f"  [Epytextç‰ˆ] å®Ÿè¡Œ: values={values}")
    print(f"  [Epytextç‰ˆ] : ã‚’ @ ã«ç½®ãæ›ãˆ")

    total = sum(values)
    mean = total / len(values)
    print(f"  [Epytextç‰ˆ] åˆè¨ˆ={total}, å¹³å‡={mean}")
    print(f"  [æ›¸ãæ›ãˆæˆåŠŸ] NumPy â†’ Epytext")
    return mean, total


# ãƒ†ã‚¹ãƒˆå®Ÿè¡Œ
print("\nâ–¼ ãƒ‘ã‚¿ãƒ¼ãƒ³3ã®ãƒ†ã‚¹ãƒˆå®Ÿè¡Œ")
values = [10.0, 20.0, 30.0]
print(f"å…¥åŠ›å€¤: values={values}\n")

print("3-0. å…ƒã®NumPy:")
calc_original_numpy(values)

print("\n3-1. NumPy â†’ PEP 257:")
calc_numpy_to_pep(values)

print("\n3-2. NumPy â†’ Google:")
calc_numpy_to_google(values)

print("\n3-3. NumPy â†’ reST:")
calc_numpy_to_rest(values)

print("\n3-4. NumPy â†’ Epytext:")
calc_numpy_to_epytext(values)


# ====================================
# æœ€çµ‚ã¾ã¨ã‚
# ====================================

